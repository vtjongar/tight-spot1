<section class="profile">
  {{!-- <h2>{{user.email}}</h2> --}}
  {{!-- <h2>{{user.email}}</h2> --}}
  {{!-- <h2>Your Dilemmas</h2> --}}
  {{#if user.dilemmas}}
    <div class="dilemmas">
      {{#each user.dilemmas}}
         {{> 'dilemma'}}
      {{/each}}
    </div>
  {{else}}
    <p>You haven't had any dilemmas yet</p>
  {{/if}}
</section>



